<?xml version="1.0" encoding="UTF-8"?>

<project>
	<pomVersion>3</pomVersion>
	<extend>../../../master/project.xml</extend>
	<name>sakai-util-id-hbm</name>
	<groupId>sakaiproject</groupId>
	<id>sakai-util-id-hbm</id>
	<currentVersion>${sakai.version}</currentVersion>
	<organization>
		<name>University of Michigan</name>
		<url>http://sakaiproject.org/</url>
	</organization>
	<inceptionYear>2003</inceptionYear>

	<properties>
		<!-- build as a jar -->
		<deploy.type>jar</deploy.type>
		<!-- deploy to "shared", "common" or "server" : shared for hibernate/spring visibility -->
		<deploy.target>shared</deploy.target>
	</properties>

	<dependencies>

		<dependency>
			<groupId>sakaiproject</groupId>
			<artifactId>sakai-util-api</artifactId>
			<version>${sakai.version}</version>
		</dependency>

		<dependency>
			<groupId>${sakai.hibernate.groupId}</groupId>
			<artifactId>${sakai.hibernate.artifactId}</artifactId>
			<version>${sakai.hibernate.version}</version>
		</dependency>

	</dependencies>

	<build>

		<sourceDirectory>src/java</sourceDirectory>

	</build>

</project>
